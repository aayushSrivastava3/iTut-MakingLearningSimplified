<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=<device-width>, initial-scale=1.0">
        <link rel="stylesheet" href="CoursesStyle.css">
    <title>Document</title>
</head>
<body>
    <h1>My courses page</h1>
    <div class="container-table">
        <table>
            <thead>
                <th>Name</th>
                <th>Email</th>
                <th>Course Title</th>
                <th>Keywords</th>
                <th>Preferences online or Offline</th>
                <th>Zoom</th>
                <th>Google Meet</th>
                <th>Others</th>
                <th>Prices</th>
            </thead>
            <tbody>
                {{#each my_courses}}
                    <tr>
                        <td>{{Name}}</td>
                        <td>{{Email}}</td>
                        <td>{{CourseTitle}}</td>
                        <td>{{Keywords}}</td>
                        <td>{{Prefer}}</td>
                        <td>{{zoom}}</td>
                        <td>{{google_meet}}</td>
                        <td>{{others}}</td>
                        <td>{{price}}</td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
        <center><a href="/"><button class="btn-primamry">Go Home</button></a></center>
        </div>

    
</body>
</html>